{
  "include": ["**/*.py"],
  "exclude": [
    ".venv",
    "venv",
    "examples",
    "docker",
    "build",
    "dist",
    "*.egg-info",
    "typings"
  ],
  "typeCheckingMode": "strict",
  "pythonVersion": "3.13",
  "venvPath": ".",
  "venv": ".venv",
  "extraPaths": ["."],
  "stubPath": "typings",
  "reportMissingModuleSource": false
}