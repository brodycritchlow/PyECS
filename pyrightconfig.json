{
  "include": ["**/*.py"],
  "exclude": [
    ".venv",
    "venv",
    "examples",
    "docker",
    "build",
    "dist",
    "*.egg-info",
    "typings",
    "tests"
  ],
  "typeCheckingMode": "all",
  "pythonVersion": "3.12",
  "extraPaths": ["."],
  "stubPath": "typings",

  "reportMissingModuleSource": false,
  "reportMissingTypeStubs": false
}
